	path models/caskami/fork
	skelmodel fork.skb
      surface body shader fork_body
      surface panel shader fork_panel01_12
      surface crane04 shader fork_crane04
      surface fl_gate shader fork_fl_gate
      surface fl_lift shader fork_fl_lift
}

animations
{
	idle     fork.skc
}

