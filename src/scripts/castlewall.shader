textures/castlewall/castlewall1

{
        qer_keyword castle
        qer_keyword castlewall
	surfaceparm stone
	surfaceparm nomarks
	{
		map textures/castlewall/castlewall1.jpg
		depthWrite
		rgbGen identity
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
	}
}

