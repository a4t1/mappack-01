textures/cmc_castle/ghost1
{
	qer_editorimage textures/cmc_castle/ghost1.tga
	qer_keyword castle
	qer_keyword ghost
	qer_keyword scary
        surfaceParm nomarks
	surfaceParm nonsolid
	surfaceParm alphashadow
	q3map_surfacelight 2000
	cull none
	nopicmip
	{
		map textures/cmc_castle/ghost1.tga
		alphaFunc GE128
		depthWrite
	nextbundle
		map $lightmap
	}
}

