textures/cmc_castle/ceiling

{
        qer_keyword castle
	surfaceparm stone
	surfaceparm nomarks
	{
		map textures/cmc_castle/ceiling.jpg
		depthWrite
		rgbGen identity
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
	}
}

