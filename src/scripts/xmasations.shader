textures/xmasations/tlighta
{
   nomipmaps
   nopicmip
   cull none
   force32bit
   surfaceparm nolightmap
   {
      animMap 02 textures/xmasations/tlighta1.tga textures/xmasations/tlighta2.tga
      rgbgen   global
      //alphagen global
      blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
   }
}
